<%= form_with model: @room do |f| %>

  <%= f.label :name,"名前" %>
  <%= f.text_field :name %>

  <%= f.label :details,"詳細" %>
  <%= f.text_field :details %>

  <%= f.label :price,"料金" %>
  <%= f.text_field :price %>

  <%= f.label :address,"住所" %>
  <%= f.text_field :address %>

  <%= f.label :image_name, "画像" %><br>
  <%= f.file_field :image_name %><br>

  <%= f.submit "更新" %>

<% end %>
<%= link_to "戻る", :back, onclick: "history.back(); return false;" %>

